        "mysql2": "^3.12.0",
        "nodemon": "^3.1.9",
        "sequelize": "^6.37.5",
        "sequelize-cli": "^6.6.2"
    },
    "devDependencies": {
        "@faker-js/faker": "^9.3.0"
    }
}
